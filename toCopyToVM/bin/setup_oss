# Setup OpenSpeedShop

oss=/opt/ossoffline_v2.2
krell=/opt/krellroot_v2.2

if [ -z "$PYTHON_DIR" ]; then
  source /opt/rh/devtoolset-2/enable
else
  unsetup python
fi

export PATH=$oss/bin:$krell/bin:${PATH}
export OPENSS_PLUGIN_PATH=$oss/lib64/openspeedshop
export LD_LIBRARY_PATH=$oss/lib64:$krell/lib64:$krell/qt3/lib64:${LD_LIBRARY_PATH}
export MANPATH=$oss/share/man:${MANPATH}
export DYNINSTAPI_RT_LIB=$krell/lib64/libdyninstAPI_RT.so
